							"name": "sinkDirectorsGold"
						},
						{
							"linkedService": {
								"referenceName": "datalake_con",
								"type": "LinkedServiceReference"
							},
							"name": "sinkTitlesGold"
						}
					],
					"transformations": [
						{
							"name": "joinDirectorTitles"
						},
						{
							"name": "selectColumns"
						},
						{
							"name": "aggregateDirectorbyshows"
						},
						{
							"name": "rankDirectors"
						},
						{
							"name": "alterRowCastGold"
						},
						{
							"name": "alterRowCategoryGold"
						},
						{
							"name": "alterRowCountriesGold"
						},
						{
							"name": "alterRowDirectorsGold"
						},
						{
							"name": "alterRowTitles"
						}
					],
					"scriptLines": [
						"source(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixCast') ~> NetflixCastGold",
						"source(output(",
						"          listed_in as string,",
						"          show_id as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixCategory') ~> NeflixCategoryGold",
						"source(output(",
						"          country as string,",
						"          show_id as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixCountries') ~> NetflixCountriesGold",
						"source(output(",
						"          director as string,",
						"          show_id as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixDirectors') ~> NetflixDirectorsGold",
						"source(output(",
						"          duration_minutes as integer,",
						"          duration_seasons as integer,",
						"          type as string,",
						"          title as string,",
						"          date_added as string,",
						"          release_year as integer,",
						"          rating as string,",
						"          description as string,",
						"          show_id as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixTitles') ~> NetflixTitlesGold",
						"source(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'delta',",
						"     fileSystem: 'silver',",
						"     folderPath: 'NetflixTitles') ~> NetflixTitlesGoldNew",
						"NetflixTitlesGold, NetflixDirectorsGold join(NetflixTitlesGold@show_id == NetflixDirectorsGold@show_id,",
						"     joinType:'inner',",
						"     matchType:'exact',",
						"     ignoreSpaces: false,",
						"     broadcast: 'auto')~> joinDirectorTitles",
						"joinDirectorTitles select(mapColumn(",
						"          duration_minutes,",
						"          duration_seasons,",
						"          type,",
						"          title,",
						"          date_added,",
						"          release_year,",
						"          rating,",
						"          description,",
						"          show_id = NetflixTitlesGold@show_id,",
						"          director",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> selectColumns",
						"selectColumns aggregate(groupBy(director),",
						"     show_count = count(show_id)) ~> aggregateDirectorbyshows",
						"aggregateDirectorbyshows rank(desc(show_count, true),",
						"     output(Top as long),",
						"     dense: true) ~> rankDirectors",
						"NetflixCastGold alterRow(insertIf(1==1)) ~> alterRowCastGold",
						"NeflixCategoryGold alterRow(insertIf(1==1)) ~> alterRowCategoryGold",
						"NetflixCountriesGold alterRow(insertIf(1==1)) ~> alterRowCountriesGold",
						"NetflixDirectorsGold alterRow(insertIf(1==1)) ~> alterRowDirectorsGold",
						"NetflixTitlesGoldNew alterRow(insertIf(1==1)) ~> alterRowTitles",
						"alterRowCastGold sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     fileSystem: 'gold',",
						"     folderPath: 'CastGold',",
						"     overwrite: true,",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sinkCastGold",
						"alterRowCategoryGold sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     fileSystem: 'gold',",
						"     folderPath: 'CategoryGold',",
						"     overwrite: true,",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sinkCategoryGold",
						"alterRowCountriesGold sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     fileSystem: 'gold',",
						"     folderPath: 'CountriesGold',",
						"     overwrite: true,",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sinkCountriesGold",
						"alterRowDirectorsGold sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     fileSystem: 'gold',",
						"     folderPath: 'DirectorsGold',",
						"     overwrite: true,",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sinkDirectorsGold",
						"alterRowTitles sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
