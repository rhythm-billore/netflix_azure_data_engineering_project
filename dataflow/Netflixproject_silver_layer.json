			"transformations": [
				{
					"name": "derivedCountryName",
					"description": "Updating country name "
				},
				{
					"name": "derivedReleaseYear",
					"description": "typecast release_year float to int"
				},
				{
					"name": "derivedMins",
					"description": "datatype change mins"
				},
				{
					"name": "alterRowCast"
				},
				{
					"name": "alterCategory"
				},
				{
					"name": "alterCountires"
				},
				{
					"name": "alterRowDirectors"
				},
				{
					"name": "alterRowTitles"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cast as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCast",
				"source(output(",
				"          listed_in as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCategory",
				"source(output(",
				"          country as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCountries",
				"source(output(",
				"          director as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixDirectors",
				"source(output(",
				"          duration_minutes as short,",
				"          duration_seasons as integer,",
				"          type as string,",
				"          title as string,",
				"          date_added as string,",
				"          release_year as integer,",
				"          rating as string,",
				"          description as string,",
				"          show_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixTitles",
				"NetflixCountries derive(country = upper(trim(country))) ~> derivedCountryName",
				"NetflixTitles derive(release_year = toInteger(release_year)) ~> derivedReleaseYear",
				"derivedReleaseYear derive(duration_minutes = toInteger(duration_minutes)) ~> derivedMins",
				"NetflixCast alterRow(upsertIf(1==1)) ~> alterRowCast",
				"NetflixCategory alterRow(upsertIf(1==1)) ~> alterCategory",
				"derivedCountryName alterRow(upsertIf(1==1)) ~> alterCountires",
				"NetflixDirectors alterRow(upsertIf(1==1)) ~> alterRowDirectors",
				"derivedMins alterRow(upsertIf(1==1)) ~> alterRowTitles",
				"alterRowCast sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'NetflixCast',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['show_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkCast",
				"alterCategory sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'NetflixCategory',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['show_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkCategory",
				"alterCountires sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'NetflixCountries',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['show_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkCountries",
				"alterRowDirectors sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'NetflixDirectors',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['show_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkDirectors",
				"alterRowTitles sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'NetflixTitles',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['show_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkTitles"
			]
		}
