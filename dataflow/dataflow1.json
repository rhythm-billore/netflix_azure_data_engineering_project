{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_cast",
						"type": "DatasetReference"
					},
					"name": "NetflixCast"
				},
				{
					"dataset": {
						"referenceName": "ds_category",
						"type": "DatasetReference"
					},
					"name": "NetflixCategory"
				},
				{
					"dataset": {
						"referenceName": "ds_countries",
						"type": "DatasetReference"
					},
					"name": "NetflixCountries"
				},
				{
					"dataset": {
						"referenceName": "ds_directors",
						"type": "DatasetReference"
					},
					"name": "NetflixDirectors"
				},
				{
					"dataset": {
						"referenceName": "ds_titles",
						"type": "DatasetReference"
					},
					"name": "NetflixTitles"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          cast as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCast",
				"source(output(",
				"          listed_in as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCategory",
				"source(output(",
				"          country as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixCountries",
				"source(output(",
				"          director as string,",
				"          show_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixDirectors",
				"source(output(",
				"          duration_minutes as short,",
				"          duration_seasons as boolean,",
				"          type as string,",
				"          title as string,",
				"          date_added as date,",
				"          release_year as short,",
				"          rating as string,",
				"          description as string,",
				"          show_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NetflixTitles"
			]
		}
	}
}